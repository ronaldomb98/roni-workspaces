{
  "include": [
    "./webpack/",
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "jsx": "react",
    "sourceMap": false,
    "target": "ES6",
    "module": "CommonJS",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "rootDir": ".",
    "esModuleInterop": true,
    "noEmit": true,
    "types": ["node", "jest"],
    "baseUrl": ".",
  },
}
